{
    "family": "sample-boot-app",
    "containerDefinitions": [
        {
            "image": "%ECS_REPO_BASE_URI%/sample-boot-app:v_%BUILD_NUMBER%",
            "name": "sample-boot-app",
            "cpu": 256,
            "memory": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ]
        }
    ]
}
